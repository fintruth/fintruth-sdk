  "repository": {
    "type": "git",
    "url": "git+https://github.com/fintruth/fintruth-sdk.git",
    "directory": "packages/client"
  },
  "scripts": {
    "build": "ts-node --files --require ./config/dotenv-safe scripts/run build",
    "build:release": "yarn build --release",
    "build:stats": "yarn build --analyze --release",
    "build:storybook": "build-storybook",
    "bundle": "ts-node --files --require ./config/dotenv-safe scripts/run bundle",
    "clean": "ts-node --files scripts/run clean",
    "copy": "ts-node --files scripts/run copy",
    "extract:fragment-types": "ts-node --files --require ./config/dotenv-safe scripts/run extract-fragment-types",
    "extract:translations": "ts-node --files --require ./config/dotenv-safe scripts/run extract-translations",
    "lint": "npm-run-all --parallel \"lint:*\"",
    "lint:files": "prettylint \"**/*.{json,md,mdx,yml}\"",
    "lint:scripts": "eslint --cache --ignore-path .gitignore --ignore-pattern \"!**/.*\" \"**/*.{js,ts,tsx}\"",
    "lint:styles": "stylelint --cache --ignore-path .gitignore --ignore-pattern \"!**/.*\" \"**/*.{ts,tsx}\"",
    "lint:types": "tsc --noEmit",
    "start": "ts-node --files --require ./config/dotenv-safe scripts/run start",
    "start:storybook": "start-storybook --port 6006",
    "test": "npm-run-all --parallel \"test:*\"",
    "test:units": "jest"
  },
