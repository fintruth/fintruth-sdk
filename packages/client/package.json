{
  "name": "@fintruth-sdk/client",
  "version": "0.1.0",
  "description": "An opinionated boilerplate for web development",
  "keywords": [
    "graphql",
    "react",
    "ssr",
    "typescript",
    "web-boilerplate"
  ],
  "homepage": "https://github.com/fintruth/fintruth-sdk/tree/develop/packages/client#readme",
