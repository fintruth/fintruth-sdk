{
  "name": "@fintruth-sdk/client",
  "private": true,
  "version": "0.0.0",
  "description": "An opinionated boilerplate for web development",
  "scripts": {
    "build": "node --require @babel/register scripts/run build",
    "build:release": "yarn build --release",
    "build:stats": "yarn build --analyze --release",
    "build:storybook": "build-storybook",
    "bundle": "node --require @babel/register scripts/run bundle",
    "clean": "node --require @babel/register scripts/run clean",
    "copy": "node --require @babel/register scripts/run copy",
    "deploy": "node --require @babel/register scripts/run deploy",
    "lint": "run-p lint:*",
    "lint:scripts": "eslint --cache --ignore-path .gitignore --ignore-pattern '!**/.*' '**/*.{js,ts,tsx}'",
    "lint:styles": "stylelint --cache --ignore-path .gitignore --ignore-pattern '!**/.*' '**/*.{ts,tsx}'",
    "lint:types": "tsc --noEmit",
    "serve": "node --require @babel/register scripts/run run-server",
    "start": "node --require @babel/register scripts/run start",
    "storybook": "start-storybook --port 6006",
    "test": "run-p test:*",
    "test:e2e": "cypress run",
    "test:units": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fintruth/fintruth-sdk.git",
    "directory": "packages/client"
  },
  "author": "Fintruth LLC",
  "license": "UNLICENSED",
  "bugs": "https://github.com/fintruth/fintruth-sdk/issues",
  "dependencies": {
