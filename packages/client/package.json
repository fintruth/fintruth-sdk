  "engines": {
    "node": ">=10.15",
    "yarn": ">=1.12"
  },
  "browserslist": [
    "> 0.20%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "stylelint",
        "jest --bail --findRelatedTests",
        "git add"
      ],
      "*.{json,md,yml}": [
        "prettier --write",
        "git add"
      ]
    }
  }
