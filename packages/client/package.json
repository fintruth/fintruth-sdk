  "private": true,
  "browserslist": [
    "> 0.20%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "stylelint",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "*.{json,md,yml}": [
